        </rdfs:subClassOf>
        <dc:identifier rdf:datatype="&xsd;string">http://www.fao.org/aims/ag_intro.htm?termid=3456&amp;myLangTerms=EN</dc:identifier>
        <dc:source rdf:datatype="&xsd;string">http://www.fao.org/aims/ag_intro.htm?termid=3456&amp;myLangTerms=EN</dc:source>
        <rdfs:comment xml:lang="en">Environment in a given area occupied by an organism or a group of organisms.</rdfs:comment>
    </Class>
    


    <!-- http://www.fao.org/aims/aos/cwr.owl#Organism -->

    <Class rdf:about="http://www.fao.org/aims/aos/cwr.owl#Organism">
