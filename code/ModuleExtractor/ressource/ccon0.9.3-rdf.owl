        <dc:source rdf:datatype="&xsd;string">http://helios.bto.ed.ac.uk/bto/glossary/ef.htm</dc:source>
        <rdfs:comment xml:lang="en">An organism having eukaryotic cells with true nuclei.</rdfs:comment>
    </Class>
    


