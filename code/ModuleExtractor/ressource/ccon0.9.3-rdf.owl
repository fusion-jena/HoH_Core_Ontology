     xmlns:skos="http://www.w3.org/2004/02/skos/core#">
    <Ontology rdf:about="http://cerrado.linkeddata.es/ecology/ccon#">
        <terms:creator rdf:datatype="&xsd;anyURI">http://lattes.cnpq.br/3259677468745429</terms:creator>
        <rdfs:comment xml:lang="en">Cerrado concepts and dynamics ontology (Ccon) aims to represent the main concepts about the Cerrado plant communities structure and dynamics.</rdfs:comment>
        <vann:preferredNamespacePrefix>ccon</vann:preferredNamespacePrefix>
        <terms:created>2013-10-31</terms:created>
        <vann:preferredNamespaceUri>http://cerrado.linkeddata.es/ecology/ccon#</vann:preferredNamespaceUri>
        <versionInfo>0.9.3</versionInfo>
        <terms:modified>2013-12-03</terms:modified>
        <terms:title xml:lang="en">The Ccon Ontology</terms:title>
        <terms:description xml:lang="en">The Ccon represents the main concepts about the Cerrado plant communities structure and dynamics. This ontology will be part of the GeoLinked Data Ontology network to enrich the web of data with geospatial information of the Cerrado Biome, especially with regard the wood plant dynamics.</terms:description>
    </Ontology>
    


