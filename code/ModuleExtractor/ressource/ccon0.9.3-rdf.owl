        </rdfs:subClassOf>
        <versionInfo rdf:datatype="&xsd;string"></versionInfo>
        <dc:source rdf:datatype="&xsd;string">http://www.fao.org/aims/ag_intro.htm?termid=2482&amp;myLangTerms=EN</dc:source>
        <dc:identifier rdf:datatype="&xsd;string">http://www.fao.org/aims/ag_intro.htm?termid=2482&amp;myLangTerms=EN</dc:identifier>
        <rdfs:comment xml:lang="en">A complex of organisms and their environment forming a functioning whole.</rdfs:comment>
    </Class>
    


