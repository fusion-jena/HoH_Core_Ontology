                <someValuesFrom rdf:resource="http://www.fao.org/aims/aos/cwr.owl#Population"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:isDefinedBy rdf:datatype="&xsd;string">The basic living system, a functional grouping of the lower-level components, including at least one cell.</rdfs:isDefinedBy>
        <dc:source rdf:datatype="&xsd;string">http://tolweb.org</dc:source>
        <rdfs:comment xml:lang="en">Any living creature. A plant, animal, or micro-organism.</rdfs:comment>
        <rdfs:comment xml:lang="pt">Um sistema vivo básico, um grupo funcional de componentes básicos, incluindo pelo menos uma célula.</rdfs:comment>
    </Class>
    


    <!-- http://www.fao.org/aims/aos/cwr.owl#Periodicity -->

