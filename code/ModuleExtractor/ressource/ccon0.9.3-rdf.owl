        <rdfs:subClassOf rdf:resource="&ccon;TropicalSeason"/>
        <rdfs:comment xml:lang="en">Consists of the period of the year in some tropical climate areas with average rainfall greater than 60mm by month.</rdfs:comment>
    </Class>
    


