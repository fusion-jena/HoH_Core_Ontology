    <!-- http://cerrado.linkeddata.es/ecology/ccon#BasalArea -->

    <Class rdf:about="&ccon;BasalArea">
        <rdfs:label rdf:datatype="&xsd;string">basal area</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.fao.org/aims/aos/cwr.owl#CommunityStructure"/>
        <rdfs:comment xml:lang="en">It&apos;s the area of a breast-high cross section of a tree or of all the trees in a stand.

Source: Merriam-Webster dictionary (www.merriam-webster.com)</rdfs:comment>
    </Class>
    


    <!-- http://cerrado.linkeddata.es/ecology/ccon#Biodiversity -->

    <Class rdf:about="&ccon;Biodiversity">
        <rdfs:label rdf:datatype="&xsd;string">biodiversity</rdfs:label>
        <rdfs:label>biological diversity</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.fao.org/aims/aos/cwr.owl#Diversity"/>
        <rdfs:comment xml:lang="en">It&apos;s the degree of veriation of life forms within a given species, ecosystem, biome, or planet.
Source: wikipedia.org</rdfs:comment>
    </Class>
    


    <!-- http://cerrado.linkeddata.es/ecology/ccon#BiologicalOrganizationLevel -->

    <Class rdf:about="&ccon;BiologicalOrganizationLevel">
        <rdfs:label rdf:datatype="&xsd;string">biological organization level</rdfs:label>
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:comment xml:lang="en">The hierarchy of complex biological structures and systems that define life using a reductionistic approach</rdfs:comment>
        <rdfs:comment xml:lang="pt">Hierarquia de estruturas e sistemas biológicos complexos que definem a vida usando uma abordagem reducionista.</rdfs:comment>
    </Class>
    


    <!-- http://cerrado.linkeddata.es/ecology/ccon#CampoCerrado -->

    <Class rdf:about="&ccon;CampoCerrado">
        <rdfs:label xml:lang="en">&quot;closed field&quot;</rdfs:label>
        <rdfs:label xml:lang="pt">campo cerrado</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ccon;Savanna"/>
        <disjointWith rdf:resource="&ccon;CampoSujo"/>
        <disjointWith rdf:resource="&ccon;CerradoSensuStricto"/>
        <rdfs:comment xml:lang="en">Scattered trees and shrubs and a large proportion of grassland</rdfs:comment>
    </Class>
    


    <!-- http://cerrado.linkeddata.es/ecology/ccon#CampoLimpo -->

    <Class rdf:about="&ccon;CampoLimpo">
        <rdfs:label rdf:datatype="&xsd;string">campo limpo</rdfs:label>
        <rdfs:label xml:lang="en">&quot;clean field&quot;</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ccon;TropicalGrassland"/>
        <rdfs:comment xml:lang="en">Dry grassland without shrubs or trees</rdfs:comment>
    </Class>
    


    <!-- http://cerrado.linkeddata.es/ecology/ccon#CampoSujo -->

    <Class rdf:about="&ccon;CampoSujo">
        <rdfs:label xml:lang="en">&quot;dirty field&quot;</rdfs:label>
        <rdfs:label xml:lang="pt">campo sujo</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ccon;Savanna"/>
        <disjointWith rdf:resource="&ccon;CerradoSensuStricto"/>
        <rdfs:comment xml:lang="en">Grassland with a scattering of shrubs and small trees</rdfs:comment>
    </Class>
    


    <!-- http://cerrado.linkeddata.es/ecology/ccon#Cerradao -->

    <Class rdf:about="&ccon;Cerradao">
        <rdfs:label rdf:datatype="&xsd;string">cerradão</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ccon;TropicalSeasonalForest"/>
        <rdfs:comment xml:lang="en">Portuguese augmentative of cerrado, is an almost closed woodland with crown cover of 50% to 90%, made up of trees, often 8-12m or even taller, casting a considerable shade so that the ground layer is much reduced</rdfs:comment>
    </Class>
    


    <!-- http://cerrado.linkeddata.es/ecology/ccon#Cerrado -->

    <Class rdf:about="&ccon;Cerrado">
        <rdfs:label rdf:datatype="&xsd;string">cerrado</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ccon;Ecoregion"/>
        <rdfs:comment xml:lang="en">It&apos;s a vast tropical savanna ecoregion of Brazil</rdfs:comment>
    </Class>
    


    <!-- http://cerrado.linkeddata.es/ecology/ccon#CerradoDeterminingFactors -->

    <Class rdf:about="&ccon;CerradoDeterminingFactors">
        <rdfs:label rdf:datatype="&xsd;string">cerrado determining factors</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ccon;Environment"/>
        <rdfs:comment xml:lang="en">Environmental factos that affect the current occurrence and geographical distribution of the cerrado ecoregion.</rdfs:comment>
    </Class>
    


    <!-- http://cerrado.linkeddata.es/ecology/ccon#CerradoSensuLato -->

    <Class rdf:about="&ccon;CerradoSensuLato">
        <rdfs:label rdf:datatype="&xsd;string">cerrado sensu lato</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ccon;Cerrado"/>
        <rdfs:comment xml:lang="en">It&apos;s a regional large scale vegetation which consists of a floristic and vegetational province</rdfs:comment>
        <rdfs:comment xml:lang="pt">É uma vegetação de larga escala regional que forma tanto uma província vegetacional quanto florística</rdfs:comment>
    </Class>
    


    <!-- http://cerrado.linkeddata.es/ecology/ccon#CerradoSensuStricto -->

    <Class rdf:about="&ccon;CerradoSensuStricto">
        <rdfs:label xml:lang="en">cerrado sensu stricto</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ccon;Savanna"/>
        <rdfs:comment xml:lang="en">The vegetation is dominated by trees and shrubs with over 30% of canopy cover, but with stiil a fair amount of herbaceous vegetation between them</rdfs:comment>
    </Class>
    


    <!-- http://cerrado.linkeddata.es/ecology/ccon#Climate -->

    <Class rdf:about="&ccon;Climate">
        <rdfs:label>climate</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ccon;CerradoDeterminingFactors"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&ccon;hasComponent"/>
                <someValuesFrom rdf:resource="&ccon;RelativeHumidity"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
